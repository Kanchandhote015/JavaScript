let data = "secret information";
class User {
    constructor(name,email) {
        this.name = name;
        this.email = email;
    }
    viewData(){
        console.log("view website data =",data);
    }
}

let student1 = new User("kanchan","abc@gamil.com");
let student2 = new User("shraddha","xyz@gmail.com");

console.log(student1);
console.log(student2);
console.log(student1.viewData());

class User1{
    constructor(name, id){
        this.name = name;
        this.id = id;
    }
}

class Admin extends User1{
    constructor(name,id){
        super(name,id);
    }

    editData() {
        console.log("To edit data");
    }
}

let person = new Admin("kanchan",78);

console.log(person);
console.log(person.editData());
